    "author": "Your Name <your-email@example.com>",
    "license": "MIT",
    "dependencies": {
        "@vercel/kv": "^3.0.0",
        "canvas": "^2.11.2",
        "express": "^4.21.1",
        "http": "0.0.1-security",
        "mathjs": "^13.2.0",
        "pg": "^8.13.0",
        "uuid": "^10.0.0",
        "winston": "^3.15.0",
        "ws": "^8.18.0",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/node": "^22.7.8",
        "c8": "^8.0.0",
        "chai": "^5.1.1",
        "mocha": "^10.7.3",
        "nodemon": "^3.1.7"
    },
    "engines": {
        "node": ">=14.0.0"
    }
